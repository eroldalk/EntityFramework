@using Skiil_CodeFirstEntity.Models.Siniflar
@model List<Yetenekler>
    @{
        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_layoutNavbar.cshtml";
    }

    <table class="table table-bordered">
        <tr>
            <th>ID</th>
            <th>Yetenek</th>
            <th>Değer</th>
            <th>Sil</th>
            <th>Güncelle</th>
        </tr>
        @foreach (var a in Model)
        {
            <tr>
                <td>@a.ID</td>
                <td>@a.ACIKLAMA</td>
                <td>@a.DEGER</td>
                <td><a href="/Admin/YetenekSil/@a.ID" class="btn btn-danger">Sil</a></td>
                <td><a href="/Admin/YetenekGüncelle/@a.ID" class="btn btn-success">Güncelle</a></td>
            </tr>
        }
    </table>
